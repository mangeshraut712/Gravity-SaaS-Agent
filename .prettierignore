# Ignore build outputs
dist
build
.next
out
.vercel
.turbo

# Ignore dependencies
node_modules
bower_components

# Ignore logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
lerna-debug.log*

# Ignore coverage reports
coverage
*.lcov
.nyc_output

# Ignore configuration files
.env
.env.*
!.env.example

# Ignore OS files
.DS_Store
Thumbs.db

# Ignore IDE files
.idea
.vscode
*.swp
*.swo
*~

# Ignore package-lock if using pnpm/yarn
package-lock.json
yarn.lock

# Ignore temporary files
*.tmp
.cache
.temp
